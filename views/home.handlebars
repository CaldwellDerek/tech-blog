{{#each allPosts as |post|}}
    <div class="post">
        <div class="post-header">
            <h3 class="post-title">{{post.title}}</h3>
            <p class="post-author">Test Author</p>
        </div>
        <div class="post-body">
            <p class="post-content">{{post.post}}</p>
        </div>
    </div>
{{/each}}
<script src="./js/home.js"></script>